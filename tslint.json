{
    "defaultSeverity": "error",
    "extends": [],
    "jsRules": {},
    "rules": {
        "quotemark": true,
        "class-name": true,
        "curly": true,
        "encoding": true,
        "eofline": true,
        "file-name-casing": [true, {".tsx": "pascal-case", ".ts": "kebab-case"}],
        "import-spacing": true,
        "indent": true,
        "interface-name": true,
        "interface-over-type-literal": true,
        "linebreak-style": [true, "LF"],
        "max-line-length": [true, 120]
    },
    "rulesDirectory": []
}